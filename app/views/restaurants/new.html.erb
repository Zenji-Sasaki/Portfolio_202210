<h1 class="title">新規レストラン登録</h1>
<%= form_with model: @restaurant do |f| %>
<%= f.hidden_field :user_id, value: current_user.id %>
<table class="box">
  <tr>
    <th><%= f.label :name , "レストラン名" %></th>
    <td><%= f.text_field :name, style: "width:100%" %></td>
  </tr>
  <tr>
    <th><%= f.label :category , "カテゴリー" %></th>
    <td><%= f.select :category, [["test1", "test1"], ["test2", "test2"]], include_blank: "選択してください" %></td>
  </tr>
</table>
<%= f.submit "登録", class: "btn btn-primary temp" %>
<% end %>
